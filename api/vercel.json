{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "*.py",
          "requirements.txt"
        ],
        "suppressStream": true
      }
    }
  ],
  "routes": [
    {
      "src": "/match-jobs",
      "methods": ["POST"],
      "dest": "app.py"
    },
    {
      "src": "/get-jobs",
      "methods": ["GET"],
      "dest": "app.py"
    },
    {
      "src": "/docs",
      "dest": "app.py"
    }
  ]
}
